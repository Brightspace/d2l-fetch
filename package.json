{
  "description": "Provides support for wrapping window.fetch calls within middleware functions",
  "keywords": [
    "fetch",
    "middleware"
  ],
  "homepage": "https://github.com/Brightspace/d2l-fetch",
  "name": "d2l-fetch",
  "main": "index.js",
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "scripts": {
    "build": "gulp",
    "pretest": "npm run build",
    "test:lint": "npm run test:lint:wc && npm run test:lint:js",
    "test:no-lint": "cross-env LAUNCHPAD_BROWSERS=firefox wct --skip-plugin sauce -p",
    "test": "npm run test:lint && npm run test:no-lint",
    "publish-location": "node publishLocation",
    "prepublish-release": "npm run build",
    "publish-release": "frau-publisher && npm run publish-location"
  },
  "config": {
    "frauPublisher": {
      "files": "./dist/**",
      "moduleType": "lib",
      "targetDirectory": "d2lfetch",
      "creds": {
        "key": "AKIAJYZ3X3B3O6UZIFAQ",
        "secretVar": "CDN_SECRET"
      },
      "devTagVar": "TRAVIS_COMMIT",
      "versionVar": "TRAVIS_TAG"
    }
  },
  "devDependencies": {
    "@babel/polyfill": "^7.0.0",
    "@webcomponents/webcomponentsjs": "^2.1.3",
    "babel-core": "^6.24.1",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.4.0",
    "babelify": "^7.3.0",
    "bower": "^1.8.2",
    "browserify": "^14.4.0",
    "cross-env": "^5.0.0",
    "deumdify": "^1.2.4",
    "eslint": "^3.19.0",
    "eslint-config-brightspace": "^0.2.4",
    "eslint-plugin-html": "^2.0.1",
    "frau-publisher": "^2.6.2",
    "gulp": "^3.9.1",
    "gulp-change": "^1.0.0",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-uglify": "^3.0.0",
    "polymer-cli": "^1.8.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "wct-browser-legacy": "^1.0.1",
    "web-component-tester": "^6.0.0"
  },
  "dependencies": {
    "frau-jwt": "^2.0.0"
  },
  "version": "",
  "resolutions": {
    "inherits": "2.0.3",
    "samsam": "1.1.3",
    "supports-color": "3.1.2",
    "type-detect": "1.0.0"
  }
